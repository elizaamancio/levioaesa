import numpy as np
import pylab as plt

# map cell to cell

points_list = [(1,0),
(3,0),
(6,0),
(0,1),
(2,1),
(3,1),
(4,1),
(6,1),
(0,2),
(1,2),
(6,2),
(0,3),
(1,3),
(4,3),
(6,3),
(3,4),
(6,4),
(8,5),
(10,5),
(0,6),
(1,6),
(2,6),
(3,6),
(4,6),
(0,7),
(5,7),
(6,7),
(10,8),
(11,9),
(12,9),
(13,9),
(5,10),
(8,10),
(9,10),
(11,10),
(12,10),
(13,10),
(12,11),
(13,11),
(11,12),
(13,12),
(9,13),
(11,13),
(8,14),
(9,14),
(10,14),
(11,14),
(12,14),
(13,14),
(7,15),
(9,15),
(11,15),
(12,15),
(13,15)]

import networkx as nx
G=nx.Graph()

G.add_edges_from(points_list)
pos = nx.spring_layout(G)
nx.draw_networkx_nodes(G,pos)
nx.draw_networkx_edges(G,pos)
nx.draw_networkx_labels(G,pos)
plt.show()
